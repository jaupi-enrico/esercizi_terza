    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="N, i, sum, num" type="Integer" array="False" size=""/>
            <output expression="&quot;Inserisci il numero di numeri: &quot;" newline="True"/>
            <input variable="N"/>
            <assign variable="sum" expression="0"/>
            <for variable="i" start="1" end="N" direction="inc" step="1">
                <output expression="&quot;Inserisci num&quot; &amp; i &amp; &quot;: &quot;" newline="True"/>
                <input variable="num"/>
                <if expression="num % 10 == 0">
                    <then>
                        <assign variable="sum" expression="sum + num"/>
                    </then>
                    <else/>
                </if>
            </for>
            <output expression="&quot;la somma dei multipli di 10 &#232; &quot; &amp; sum" newline="True"/>
        </body>
    </function>
